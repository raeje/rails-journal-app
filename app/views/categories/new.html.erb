<h1>Create Category</h1>
<%= form_with(model: @category, url: categories_url, method: :post, local: true) do |f| %>
  <div>
    <%= f.label :name, "custom category name" %>
    <%= f.text_field :name  %>
  </div>


  <div>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>

  <%= f.submit %>
<% end %>